{
  "name": "obsidian-super-plan",
  "version": "0.0.4",
  "description": "This is a sample plugin for Obsidian (https://obsidian.md)",
  "main": "main.js",
  "scripts": {
    "dev": "npm run clear && cross-env NODE_ENV=development run-p dev:*",
    "dev:js": "npm run build:js -- --mode development",
    "dev:web": "vite dev --config vite.config.windows.ts",
    "build": "cross-env NODE_ENV=production run-s clear build:js build:web",
    "build:js": "vite build --config vite.config.ts",
    "build:web": "vite build --config vite.config.windows.ts",
    "release": "run-s build pack",
    "pack": "rimraf plugin.zip && cd dist/ && bestzip ../plugin.zip * ../main.js ../styles.css ../manifest.json",
    "clear": "rimraf dist main.js styles.css",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@codemirror/state": "0.19.0",
    "@codemirror/view": "0.19.29",
    "@sveltejs/vite-plugin-svelte": "^1.4.0",
    "@tsconfig/svelte": "^3.0.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "bestzip": "^2.2.1",
    "builtin-modules": "3.3.0",
    "cross-env": "^7.0.3",
    "electron": "18.3.15",
    "esbuild": "0.14.47",
    "esbuild-svelte": "^0.7.3",
    "npm-run-all": "^4.1.5",
    "obsidian": "latest",
    "prettier": "^2.8.2",
    "rimraf": "^4.1.1",
    "svelte": "^3.55.0",
    "svelte-preprocess": "^5.0.0",
    "tslib": "2.4.0",
    "typed-event-emitter": "^3.0.0",
    "typescript": "4.7.4",
    "vite": "^4.0.4"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "@tgrosinger/md-advanced-tables": "^3.8.0",
    "lodash-es": "^4.17.21"
  }
}